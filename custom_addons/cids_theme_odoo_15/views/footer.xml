<odoo>
    <template id="custom_footer" inherit_id="website.layout" name="Custom Footer">
        <xpath expr="//footer" position="replace">
            <footer>
                <style>
                    :root {
                        --font-size-1:0.06vw;
                    }
                    .site-map {
                        width:90%;
                        margin:auto;
                        margin-top:var(--font-size-60);
                        margin-bottom:var(--margin-70);
                    }
                    .site-map h1{
                        font-family: 'Lorin';
                        font-style: normal;
                        font-weight: 400;
                        font-size:var(--font-size-30);
                        line-height: 39px;
                        text-transform: capitalize;
                        color: #50575D;
                        margin-bottom:var(--font-size-32);

                    }
                    .sitemap-inner{
                        display:flex;
                        justify-content: space-between;
                    }
                    .sitemap-inner >div{
                        width:18%;
                        position:relative;
                        gap:var(--font-size-20);
                        padding-top:var(--font-size-24);
                    }
                    .sitemap-inner >div:nth-child(2){
                        display:flex;
                        flex-direction: column;
                    }

                    .sitemap-inner >div:nth-child(2) a  {
                        font-family: 'Lorin';
                        font-style: normal;
                        font-weight: 300;
                        font-size:var(--font-size-14);
                        line-height: 14px;
                        text-transform: capitalize;
                        color: #50575D;
                    }

                    .sitemap-inner >div::after  {
                        content:"";
                        position: absolute;
                        left:0;
                        right:0;
                        top:0px;
                        background: #50575D;
                        opacity: 0.2;
                        height:var(--font-size-1);
                    }
                    .sitemap-inner >div::before{
                        content:"";
                        position: absolute;
                        left:0;
                        top:0px;
                        width:var(--font-size-50);
                        background:#FFBC00;
                        height:var(--font-size-1);
                    }
                    .sitemap-inner >div a:nth-child(1)   {
                        font-family: 'Lorin';
                        font-style: normal;
                        font-weight: 700;
                        font-size:var(--font-size-16);
                        line-height: 16px;
                        text-transform: capitalize;
                        color: #50575D;
                    }
                    .sitemap-inner >div a{
                        text-decoration: none;
                    }
                    @media screen and (max-width:767px)
        {
            .site-map h1{
                font-size:20px;
            }
            .sitemap-inner{
                flex-wrap:wrap;
            }
            .sitemap-inner >div a{
                font-size:14px!important;
            }
            .sitemap-inner >div{
                width:30%;
            }
            .sitemap-inner >div::before{
                width:50px;
            }
        }
        @media screen and (max-width:580px)
        {
            .sitemap-inner >div{
                width:48%;
                margin-bottom:15px;
            }
            .site-map h1{
                font-size:16px;
            }
        }
        @media screen and (max-width:380px)
        {
            .sitemap-inner >div{
                width:100%;
                margin-bottom:15px;
            }
        }
                </style>
